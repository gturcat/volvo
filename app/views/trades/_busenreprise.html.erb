<div class="tab">
  <div class="panel panel-default">
    <!-- Default panel contents -->
    <div class="panel-heading">Bus liés à la commande</div>
    <!-- Table -->
    <table class="table">
      <thead>
        <tr>
          <th>Marque</th>
          <th>type</th>
          <th>Long. </th>
          <th>Places</th>
          <th>V.C.L</th>
          <th>N° Chassis</th>
        </tr>
      </thead>
      <tbody>
        <tr>
            <td><%= @traded_bus.marque %></td>
            <td><%= @traded_bus.type.name  if @traded_bus.type.present?  %></td>
             <td><%= @traded_bus.longueur %></td>
            <td><%= @traded_bus.nbr_places %></td>
            <td> <%= @traded_bus.volo_coach_line %></td>
            <td> <%= @traded_bus.numero_chassis %></td>
        </tr>
      </tbody>
    </table>
  </div>
</div>
